[MASTER]

jobs=4


[MESSAGES CONTROL]

disable=fixme,invalid-sequence-index


[REPORTS]

output-format=parseable
reports=yes
score=yes


[REFACTORING]

max-nested-blocks=5


[BASIC]

include-naming-hint=yes


[FORMAT]

expected-line-ending-format=LF
max-line-length=120


[MISCELLANEOUS]

notes=FIXME,XXX,TODO,HACK


[CLASSES]

exclude-protected=_pattern_type


[DESIGN]

min-public-methods=0


[IMPORTS]

allow-wildcard-with-all=no


[EXCEPTIONS]

overgeneral-exceptions=
